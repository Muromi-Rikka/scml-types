{
  "$schema": "https://turborepo.dev/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["download", "^build"],
      "inputs": ["remote/**", "tsdown.config.ts", "tsconfig.json"],
      "outputs": ["type-dist/**"]
    },
    "download": {
      "outputs": ["remote/**"]
    },
    "typecheck": {
      "dependsOn": ["^build"]
    }
  }
}
